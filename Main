
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local dfhrpsize
for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
if v.Name == "HumanoidRootPart" then
dfhrpsize = v.Size
end
end
if dfhrpsize == nil then
dfhrpsize = Vector3.new(2,2,1)
end

local Window = Rayfield:CreateWindow({
	Name = "Nic's Sword Fighting",
	LoadingTitle = "Thanks For Choosing Us",
	LoadingSubtitle = "Made by wowzers!#0560",
	ConfigurationSaving = {
		Enabled = true,
		FileName = "Nic SF"
	},
	KeySystem = false, 
	KeySettings = {
		Title = "Sirius Hub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/sirius)",
		Key = "ABCDEF"
	}
})
local Tab = Window:CreateTab("Touch Interest Method")
local HB = Window:CreateTab("Hitbox Method")
local BM = Window:CreateTab("Bring Method")
local CM = Window:CreateTab("Character Mods")
local B
local Paragraph = BM:CreateParagraph({Title = "Bring", Content = "Although Nobody Can See When You Are Bringing, It Can Be Effective For Most Hit Registration Systems Are Client Sided"})
local Bring = BM:CreateToggle({
	Name = "Bring All",
	CurrentValue = false,
	Flag = "BA", 
	Callback = function(Value)
B = Value
	end,
})
local Paragraph = HB:CreateParagraph({Title = "Hitboxes.", Content = "Hitboxes are very useful. they are less detectable. and overall sometimes just better."})

local ka
local ex
local kilp
local visamt = .5
local expandamt = 10
local vis
local whitelisted = {}
local Paragraph = Tab:CreateParagraph({Title = "Note:", Content = "Touch Interests May Have Checks For Distance, and can be detected easily. However it is a very effective method for kill alls and more."})
local HBETOG = HB:CreateToggle({
	Name = "Hitboxes",
